{
  "datasets": [
    {
      "name": "0608be9d",
      "displayName": "Global Carbon Emission Points Dataset",
      "queryLines": [
        "SELECT latitude,\r\n",
        "longitude,\r\n",
        "`GHG emissions mtons CO2e` as Emissions\r\n",
        "FROM emissions.default.emissions_data"
      ]
    },
    {
      "name": "b73952d6",
      "displayName": "Emissions per Person",
      "queryLines": [
        "SELECT county_state_name,\r\n",
        "       population,\r\n",
        "       CAST(\r\n",
        "           REPLACE(`GHG emissions mtons CO2e`, ',', '')\r\n",
        "           AS DOUBLE\r\n",
        "       ) / NULLIF( CAST(\r\n",
        "           REPLACE(population, ',', '')\r\n",
        "           AS DOUBLE\r\n",
        "       ), 0 \r\n",
        "       )AS Emissions_per_person\r\n",
        "FROM emissions.default.emissions_data\r\n",
        "ORDER BY emissions_per_person DESC"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "4139e708",
      "displayName": "Total Emissions Per State",
      "queryLines": [
        "SELECT state_abbr,\r\n",
        "       SUM(CAST(\r\n",
        "           REPLACE(`GHG emissions mtons CO2e`, ',', '')\r\n",
        "           AS DOUBLE\r\n",
        "       )) AS Total_Emissions\r\n",
        "FROM emissions_data\r\n",
        "GROUP BY state_abbr\r\n",
        "ORDER BY Total_Emissions DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "b83282e2",
      "displayName": "Top Counties Ranked by Carbon Emissions and Population",
      "queryLines": [
        "SELECT county_state_name,\r\n",
        "       population,\r\n",
        "       CAST(\r\n",
        "        REPLACE(`GHG emissions mtons CO2e`,',','')\r\n",
        "        AS DOUBLE\r\n",
        "       ) AS Total_Emissions\r\n",
        "FROM emissions_data\r\n",
        "ORDER BY total_emissions DESC\r\n",
        "LIMIT 10\r\n"
      ],
      "catalog": "emissions",
      "schema": "default"
    }
  ],
